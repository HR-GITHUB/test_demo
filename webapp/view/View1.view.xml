<mvc:View
    controllerName="vpndemo.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content />
                    <Table id="table2" items="{DataModelSF>/}" class="sapUiSizeCompact">
<columns>
<Column><Label text="Customer Name"/></Column>
<Column><Label text="City"/></Column>
<Column><Label text="Organization"/></Column>
<Column><Label text="Age"/></Column>
<Column><Label text="Country"/></Column>
<Column><Label text="State"/></Column>
</columns>
<ColumnListItem>
<Text text="{DataModel>CUSTOMER_NAME}"/>
<Text text="{DataModel>CITY}"/>
<Text text="{DataModel>ORGANIZATION}"/>
<Text text="{DataModel>AGE}"/>
</ColumnListItem>
</Table>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
